  "bugs": {
    "url": "https://github.com/azupatrick0/restcountries/issues"
  },
  "homepage": "https://github.com/azupatrick0/restcountries#readme",
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.6",
